{
    "devCommand": "pipenv run dev",
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "installCommand": "pip3 install --user pipenv && pipenv install",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}
