{
    "devCommand": "pipenv run dev",
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        }
    ],
    "buildCommand": "gcc submodules/svgshift/svgshift.c -o submodules/svgshift/svgshift",
    "installCommand": "pipenv install",
    "routes": [
        {
            "src": "/(.*)",
            "dest": "main.py"
        }
    ]
}
